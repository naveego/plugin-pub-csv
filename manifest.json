{
    "name": "pub-csv",
    "displayName": "CSV Publisher",
    "description": "Publisher which extracts data from CSV (RFC 4180) files.",
    "versionDescription": "Version 1",
    "version": "2.0.0",
    "iconFile": "icon.png",
    "executable": "plugin-pub-csv.exe",
    "kind": "publisher",
    "configSchema": {
        "ui": {
            "ui:order": [
                "path",
                "hasHeader",
                "delimiter",
            ]
        },
        "schema": {
            "type": "object",
            "required": [
                "path",
                "delimiter"
            ],
            "properties": {
                "path": {
                    "type": "string",
                    "title": "Path",
                    "description": "Absolute path to file(s), supports *"
                },
                "hasHeader": {
                    "type": "boolean",
                    "title": "Has Header",
                    "description": "Check this box if the CSV file has a header line."
                },
                "delimiter": {
                    "type": "string",
                    "title": "Delimiter",
                    "default": ","
                }
            }
        }
    }
}